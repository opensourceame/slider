[sub_resource type="GDScript" id="GDScript_1x8h2"]
script/source = "extends Control

func _ready():
	$VBoxContainer/Grid3x5Button.pressed.connect(_on_grid_3x5_pressed)
	$VBoxContainer/Grid4x6Button.pressed.connect(_on_grid_4x6_pressed)
	$VBoxContainer/Grid5x8Button.pressed.connect(_on_grid_5x8_pressed)
	$VBoxContainer/QuitButton.pressed.connect(_on_quit_pressed)

func _on_grid_3x5_pressed():
	GameState.set_grid_size(Vector2i(3, 5))
	get_tree().change_scene_to_file(\"res://scenes/Game.tscn\")

func _on_grid_4x6_pressed():
	GameState.set_grid_size(Vector2i(4, 6))
	get_tree().change_scene_to_file(\"res://scenes/Game.tscn\")

func _on_grid_5x8_pressed():
	GameState.set_grid_size(Vector2i(5, 8))
	get_tree().change_scene_to_file(\"res://scenes/Game.tscn\")

func _on_quit_pressed():
	get_tree().quit()
"

